		stack =  load('../data/Result.txt','ascii');
         R=143;
         R=116;
         Z=93;
%                   R=12;
%          C=12;
%          Z=12;
         stack=reshape(stack,R,C,Z);
		 [X,Y,Z] = meshgrid([1:R],[1:C],[1:Z]);
		 xslice = [1,round(R/2),R];   
		 yslice = [];
		 zslice = round(R/2);
		 slice(X,Y,Z,stack,xslice,yslice,zslice);
